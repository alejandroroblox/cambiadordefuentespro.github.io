{
    "manifest_version": 3,
    "name": "Selector de Fuente Dinámico",
    "version": "1.1",
    "description": "Elige tu propio archivo .ttf desde el menú de la extensión.",
    "permissions": ["storage"],
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"],
            "run_at": "document_start"
        }
    ]
}
